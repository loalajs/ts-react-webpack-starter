  "description": "React, React Router 4.0 and Webpack 3 Starters",
  "main": "dist/server/start.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/**/*",
    "webpack-dev-server": "node_modules/.bin/webpack-dev-server --color --progress --open --config ./src/client/config/webpack.config.dev.js",
