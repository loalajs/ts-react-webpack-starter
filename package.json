{
  "name": "react-ts-starter",
  "version": "1.0.0",
  "description": "React, React Router 4.0 and Webpack 3 Starters",
  "main": "dist/server/start.js",
  "types": "dist/src/config/misc.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server-clean": "rm -rf dist/server/**/*",
    "client-clean": "rm -rf dist/client/**/*",
    "client-dev": "node_modules/.bin/webpack-dev-server --color --progress --open --config ./src/client/config/webpack.config.dev.js",
    "server-build": "npm run server-clean && NODE_ENV=production gulp default",
    "server-build:watch": "npm run server-clean && NODE_ENV=development gulp watch",
    "client-build:watch": "npm run client-clean && node_modules/.bin/webpack --progress --config src/client/config/webpack.config.dev.js",
    "client-build": "npm run client-clean && NODE_ENV=production node_modules/.bin/webpack --progress --config src/client/config/webpack.config.prod.js",
    "nodemon": "NODE_ENV=development node_modules/.bin/nodemon --watch dist/",
    "start": "NODE_ENV=production node dist/server/start.js"
  },
  "author": "Loala.js",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.2.2",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "jsonwebtoken": "^8.2.0",
    "material-ui": "^1.0.0-beta.26",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "reflect-metadata": "^0.1.12",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "typeorm": "^0.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/express": "^4.11.0",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.6",
    "@types/passport": "^0.4.3",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-loadable": "^5.3.0",
    "@types/react-router-dom": "^4.2.3",
